<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jor.ebooklib.domain.Admin" table="admin">
		<id name="admin_id" column="admin_id">
			<generator class="native"></generator>
		</id>
		
		<property name="admin_account" />
		<property name="admin_password" />
		<property name="admin_name" />
		<property name="admin_phone" />
		<property name="admin_image" />
				
	</class>
</hibernate-mapping>